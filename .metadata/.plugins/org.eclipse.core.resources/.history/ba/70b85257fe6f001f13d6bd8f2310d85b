package Encapsulation;

public class Employ {
String name;
int id;
double sal;

// -->getters
public String getName() {
	//validation
	return name;
}
public int getId() {
	//validation
	return id;
}
public double getSal() {
	// validation
	return sal;
}

//----> setters
public void setName(String name) {
	//validation
	this.name = name;
	
}
public void setId(int id) {
	//validation
	this.id = id;
}
public void setSal(double sal) {
	//validation
	this.sal= sal;
}



	
	
	

}
